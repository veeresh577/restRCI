{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "ErrID",
    "Report"
  ],
  "properties": {
    "ErrID": {
      "$id": "#/properties/ErrID",
      "type": "integer",
      "title": "The Errid Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "Report": {
      "$id": "#/properties/Report",
      "type": "string",
      "title": "The Report Schema",
      "default": "",
      "examples": [
        "SetGPIOs"
      ],
      "pattern": "^(.*)$"
    }
  }
}